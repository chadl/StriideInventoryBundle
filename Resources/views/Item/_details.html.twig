<table class="table table-bordered table-condensed">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
        <tr>
            <th>Quantity</th>
            <td>{{ entity.quantity }}</td>
        </tr>
        <tr>
            <th>Description</th>
            <td>{{ entity.description }}</td>
        </tr>
        <tr>
            <th>Type</th>
            <td>{{ entity.type }}</td>
        </tr>
    </tbody>
</table>

{% render 'StriideInventoryBundle:Media:show' with {'id': entity.photo } %}
